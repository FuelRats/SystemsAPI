LOAD CSV
    FROM 'planets.csv'
    INTO {{db_uri}}
    TARGET TABLE bodies

    WITH truncate,
            drop indexes,
            disable triggers,
            skip header = 1,
            fields optionally enclosed by '"',
            fields escaped by backslash-quote,
            fields terminated by ',',
            on error resume next
    SET work_mem to '{{work_mem}}', maintenance_work_mem to '{{maintenance_work_mem}}';
